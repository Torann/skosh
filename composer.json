{
  "name": "torann/skosh",
  "description": "Skosh is a simple static site generator for PHP using Gulp.",
  "keywords": ["static","generator","grunt","jekyll","static site"],
  "license": "BSD-2-Clause",
  "type": "project",
  "require": {
    "torann/skosh-generator": "0.2.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env.yml');\"",
      "php -r \"copy('.remote.example', '.remote.yml');\""
    ]
  }
}
